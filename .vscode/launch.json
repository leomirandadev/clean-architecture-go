{
  "version":"1.0.0",
  "configurations": [
      {
          "name":"Debugging Go",
          "type":"go",
          "request":"launch",
          "mode":"auto",
          "program":"${workspaceFolder}/cmd/${input:goService}/main.go",
          "cwd": "${workspaceFolder}"
      },
  ],
  "inputs": [
      {
          "id":"goService",
          "type":"pickString",
          "description":"Please choose a go service name",
          "options":[
              "api",
          ],
      }
  ]
}